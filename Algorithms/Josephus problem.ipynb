    "def kill_people(n, k):\n",
    "    #n: int, number of total people\n",
    "    #k: the killing frequency\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return (kill_people(n-1,k)+k-1)%n + 1 #%n to reset location"
   ]
  },
